<% content_for(:corpo) do %>
<div class="block">
	<div class="content">
		<div class="inner">
			<h2>Editando Registro de voto</h2>

			<% form_for(@registro_de_voto) do |f| %>
			<%= f.error_messages %>

			<%= render :partial => "form", :locals=>{:formulario=>f}%>

			<%= f.submit 'Update' %>

			<%= link_to 'Show', @registro_de_voto %> |
			<%= link_to 'Back', registro_de_votos_path %>

		</div>
	</div>
</div>
<%end%>
